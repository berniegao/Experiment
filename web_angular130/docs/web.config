<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <rewrite>
            <rules>
                <rule name="http://localhost:9002/examples/example-ngView-directive" enabled="true">
                    <match url="examples/example-ngView-directive/[a-z0-9\-/]*(?!js)$" />
                    <action type="Rewrite" url="examples/example-ngView-directive/" />
                </rule>
                <rule name="angular-ui-router">
                    <match url="angular-ui-router/sample/[a-z0-9\-/]*(?!js)$" />
                    <action type="Rewrite" url="angular-ui-router/sample/" />
                </rule>
            </rules>
        </rewrite>
    </system.webServer>
</configuration>
