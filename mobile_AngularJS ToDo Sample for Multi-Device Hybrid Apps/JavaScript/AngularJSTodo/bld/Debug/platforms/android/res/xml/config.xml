<?xml version='1.0' encoding='utf-8'?>
<widget id="com.microsoft.mdha.samples.angularjstodo" version="1.0.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps">
    <preference name="loglevel" value="DEBUG" />
    <preference name="AndroidLaunchMode" value="singleTop" />
    <feature name="App">
        <param name="android-package" value="org.apache.cordova.App" />
    </feature>
    <feature name="InAppBrowser">
        <param name="android-package" value="org.apache.cordova.inappbrowser.InAppBrowser" />
    </feature>
    <name>AngularjsToDo</name>
    <description>
    A blank project that uses Apache Cordova to help you build an app that targets multiple mobile platforms: Android, iOS, Windows, and Windows Phone.
  </description>
    <author email="multidevicehybridapp@microsoft.com" href="http://go.microsoft.com/fwlink/?LinkID=398477">
    Multi-Device Hybrid Apps Team
  </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="SplashScreen" value="screen" />
    <preference name="BackgroundColor" value="#EDECEC" />
    <vs:features>
        <vs:feature>org.apache.cordova.geolocation@0.3.7</vs:feature>
        <vs:feature>com.msopentech.azure-mobile-services@0.1.2</vs:feature>
    </vs:features>
    <vs:platformSpecificValues>
        <vs:platformSpecificWidget platformName="windows8" />
    </vs:platformSpecificValues>
</widget>
